<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<beans profile="desktop">

		<bean id="smart.context.base" class="org.carewebframework.smart.SmartContextBase"
			abstract="true" init-method="init" destroy-method="destroy">
			<property name="eventManager" ref="eventManager" />
		</bean>

		<bean id="smart.context.user" parent="smart.context.base"
			class="org.hspconsortium.cwf.api.smart.SmartContextUser" depends-on="userContext" />

		<bean id="smart.context.patient" parent="smart.context.base"
			class="org.hspconsortium.cwf.api.smart.SmartContextPatient" depends-on="patientContext" />

	</beans>
</beans>
